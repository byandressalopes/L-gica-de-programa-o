algoritmo "contas_subtracao"
var
contador: inteiro
numero1, numero2, resultado: real

inicio
para contador de 1 ate 3 faca

escreval("Conta ", contador, " - Insira o numero: ")
leia(numero1)
escreval("Conta ", contador, " - Insira o numero: ")
leia(numero2)

resultado <- numero1 - numero2                        l
escreval("Resultado da subtração: ", numero1, " - " , numero2, " = " , resultado)
escreval("*************************")
fimpara

fimalgoritmo
